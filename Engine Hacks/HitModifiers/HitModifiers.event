#ifndef HIT_MODIFIERS
#define HIT_MODIFIERS

PUSH
  ORG 0x2A558
  jumpToHack(HitModifiers)
POP

ALIGN 4
MESSAGE Hit Modifiers at currentOffset
HitModifiers:
#incbin "HitModifiers.dmp"
WORD 0xEC //1RN mode global flag ID
WORD 0xED //Fates RN mode global flag ID
WORD 0xEE //Nice RNG mode global flag ID

#endif //HIT_MODIFIERS
