--- /dev/null
+++ b/Engine Hacks/Skill System/New Skills/Puissance.s	
@@ -0,0 +1,35 @@
+.thumb
+.equ PuissanceID, SkillTester+4
+
+push {r4-r7, lr}
+mov r4, r0 @atkr
+mov r5, r1 @dfdr
+
+@hp not at full
+ldrb r0, [r4, #0x14] @attacker str
+ldrb r1, [r5, #0x14] @defender str
+cmp r0, r1
+ble End @skip if str is less or equal
+
+@has Puissance
+ldr r0, SkillTester
+mov lr, r0
+mov r0, r4 @attacker data
+ldr r1, PuissanceID
+.short 0xf800
+cmp r0, #0
+beq End
+
+@add 3 damage
+mov r1, #0x5a
+ldrh r0, [r4, r1] @atk
+add r0, #3
+strh r0, [r4,r1]
+
+End:
+pop {r4-r7, r15}
+.align
+.ltorg
+SkillTester:
+@Poin SkillTester
+@WORD PuissanceID
